<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.kuklinski.clientsManagement.controllers.ListClientsController">
   <children>
      <HBox prefHeight="62.0" prefWidth="677.0" spacing="20.0">
         <children>
            <Button maxHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#addClient" prefHeight="200.0">
               <graphic>
                  <ImageView fitHeight="57.0" fitWidth="66.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icons/person.png" />
                     </image>
                     <effect>
                        <SepiaTone />
                     </effect>
                  </ImageView>
               </graphic>
            </Button>
            <Label text="Filtry" />
            <Button mnemonicParsing="false" text="Button" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <TableView fx:id="clientsTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="75.0" text="%name.general" />
          <TableColumn fx:id="surnameColumn" prefWidth="75.0" text="%surname.general" />
            <TableColumn fx:id="peselColumn" prefWidth="75.0" text="%pesel.client" />
            <TableColumn fx:id="clickAmountColumn" prefWidth="75.0" text="%clickAmount.client" />
            <TableColumn fx:id="phoneColumn" prefWidth="75.0" text="%phone.client" />
            <TableColumn fx:id="cityColumn" prefWidth="75.0" text="%city.client" />
            <TableColumn fx:id="adviserColumn" prefWidth="75.0" text="%adviser.client" />
            <TableColumn fx:id="incomeColumn" prefWidth="75.0" text="%income.client" />
            <TableColumn fx:id="detailsColumn" prefWidth="75.0" text="%details.general" />
        </columns>
      </TableView>
   </children>
</VBox>
